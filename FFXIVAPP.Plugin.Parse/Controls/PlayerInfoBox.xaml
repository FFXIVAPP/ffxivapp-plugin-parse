<UserControl x:Class="FFXIVAPP.Plugin.Parse.Controls.PlayerInfoBox"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:CommonConverters="clr-namespace:FFXIVAPP.Common.Converters;assembly=FFXIVAPP.Common"
             xmlns:Custom="http://metro.mahapps.com/winfx/xaml/shared"
             xmlns:Parse="clr-namespace:FFXIVAPP.Plugin.Parse"
             xmlns:PluginProperties="clr-namespace:FFXIVAPP.Plugin.Parse.Properties"
             xmlns:converters="clr-namespace:FFXIVAPP.Plugin.Parse.Converters"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d">

    <UserControl.Resources>
        <CommonConverters:VisibilityConverter x:Key="VisibilityConverter" />
        <x:Static x:Key="Settings" Member="PluginProperties:Settings.Default" />
        <Custom:ToUpperConverter x:Key="ToUpperConverter" />
        <x:Static x:Key="PluginViewModel" Member="Parse:PluginViewModel.Instance" />
        <converters:NameToAvatarConverter x:Key="NameToAvatarConverter" />
        <converters:JobToIconConverter x:Key="JobToIconConverter" />
        <Style x:Key="StatLabelStyle">
            <Setter Property="Control.Padding" Value="0" />
            <Setter Property="FrameworkElement.HorizontalAlignment" Value="Left" />
        </Style>
        <Style x:Key="CommaNumberStyle" BasedOn="{StaticResource StatLabelStyle}">
            <Setter Property="ContentControl.ContentStringFormat" Value="{}{0:N0}" />
        </Style>
        <Style x:Key="PercentStyle" BasedOn="{StaticResource StatLabelStyle}">
            <Setter Property="ContentControl.ContentStringFormat" Value="{}{0:P}" />
        </Style>
        <Style x:Key="WhiteStatTextBlockStyle">
            <Setter Property="Control.Padding" Value="0" />
            <Setter Property="Control.Foreground" Value="White" />
            <Setter Property="FrameworkElement.HorizontalAlignment" Value="Left" />
        </Style>
    </UserControl.Resources>

    <Grid x:Name="LayoutRoot"
          Width="270"
          Margin="5">
        <Grid.Triggers>
            <EventTrigger RoutedEvent="FrameworkElement.Loaded">
                <BeginStoryboard x:Name="LayoutRootBeginStoryBoard">
                    <Storyboard x:Name="LayoutRootStoryBoard">
                        <DoubleAnimation AutoReverse="false"
                                         BeginTime="0:0:0"
                                         Storyboard.TargetName="LayoutRoot"
                                         Storyboard.TargetProperty="(UIElement.Opacity)"
                                         From="0"
                                         To="1"
                                         Duration="0:0:.5" />
                    </Storyboard>
                </BeginStoryboard>
            </EventTrigger>
        </Grid.Triggers>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="5" />
            <ColumnDefinition Width="5" />
            <ColumnDefinition Width="40" />
            <ColumnDefinition Width="*" />
            <ColumnDefinition Width="5" />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="5" />
            <RowDefinition Height="5" />
            <RowDefinition Height="20" />
            <RowDefinition Height="25" />
            <RowDefinition Height="*" />
            <RowDefinition Height="5" />
        </Grid.RowDefinitions>
        <Border Grid.Row="0"
                Grid.RowSpan="3"
                Grid.Column="0"
                Grid.ColumnSpan="5"
                Background="{DynamicResource AccentColorBrush}"
                BorderBrush="{DynamicResource AccentColorBrush}"
                BorderThickness="1" />
        <Border Grid.Row="1"
                Grid.RowSpan="3"
                Grid.Column="2"
                Width="40"
                Height="40"
                Background="White"
                BorderBrush="{DynamicResource AccentColorBrush}"
                BorderThickness="1,1,1,1">
            <Image RenderOptions.BitmapScalingMode="HighQuality">
                <Image.Source>
                    <MultiBinding Converter="{StaticResource NameToAvatarConverter}">
                        <Binding RelativeSource="{RelativeSource Self}" />
                        <Binding Path="Name" />
                    </MultiBinding>
                </Image.Source>
            </Image>
        </Border>
        <DockPanel Grid.Row="0"
                   Grid.RowSpan="3"
                   Grid.Column="3"
                   LastChildFill="True">
            <DockPanel DockPanel.Dock="Left">
                <Image Width="24"
                       Height="24"
                       Margin="5,0"
                       RenderOptions.BitmapScalingMode="HighQuality"
                       Source="{Binding NPCEntry.Job, Converter={StaticResource JobToIconConverter}}"
                       Stretch="UniformToFill" />
            </DockPanel>
            <DockPanel DockPanel.Dock="Right">
                <TextBlock Margin="5,0"
                           VerticalAlignment="Center"
                           FontSize="20"
                           Style="{StaticResource WhiteStatTextBlockStyle}"
                           Text="{Binding ActivePercent, StringFormat=\{0:P0\}}" />
                <TextBlock Margin="4"
                           Padding="0"
                           VerticalAlignment="Bottom"
                           Style="{StaticResource WhiteStatTextBlockStyle}"
                           Text="{Binding Locale[parse_ActiveText], Source={StaticResource PluginViewModel}, Converter={StaticResource ToUpperConverter}}" />
                <TextBlock Margin="5,0"
                           VerticalAlignment="Center"
                           FontSize="20"
                           Style="{StaticResource WhiteStatTextBlockStyle}"
                           Text="{Binding CombinedDPS, StringFormat=\{0:N0\}}"
                           ToolTip="(DPS + DOTPS)" />
                <TextBlock Margin="4"
                           Padding="0"
                           VerticalAlignment="Bottom"
                           Style="{StaticResource WhiteStatTextBlockStyle}"
                           Text="DPS" />
            </DockPanel>
            <DockPanel />
        </DockPanel>
        <TextBlock Grid.Row="3"
                   Grid.Column="3"
                   Padding="5,0"
                   VerticalAlignment="Center"
                   Foreground="{DynamicResource LabelTextBrush}"
                   Text="{Binding Name, Converter={StaticResource ToUpperConverter}}" />
        <Border Grid.Row="3"
                Grid.Column="0"
                Grid.ColumnSpan="5"
                BorderBrush="{DynamicResource AccentColorBrush}"
                BorderThickness="1,0" />
        <Border Grid.Row="4"
                Grid.RowSpan="3"
                Grid.Column="0"
                Grid.ColumnSpan="5"
                BorderBrush="{DynamicResource AccentColorBrush}"
                BorderThickness="1,0,1,1">
