                                       Text="{Binding CombinedDamageTakenResistModAverage, StringFormat={}{0:N0}}"
                                       Visibility="{Binding ShowBasicCombinedDamageTakenResistModAverage, Source={StaticResource Settings}, Converter={StaticResource VisibilityConverter}}" />
                            <TextBlock Foreground="{DynamicResource LabelTextBrush}"
                                       Text="{Binding CombinedDamageTakenEvade, StringFormat={}{0:N0}}"
                                       Visibility="{Binding ShowBasicCombinedDamageTakenEvade, Source={StaticResource Settings}, Converter={StaticResource VisibilityConverter}}" />
                            <TextBlock Foreground="{DynamicResource LabelTextBrush}"
                                       Text="{Binding CombinedDamageTakenEvadePercent, StringFormat={}{0:P}}"
                                       Visibility="{Binding ShowBasicCombinedDamageTakenEvadePercent, Source={StaticResource Settings}, Converter={StaticResource VisibilityConverter}}" />
                            <TextBlock Foreground="{DynamicResource LabelTextBrush}"
                                       Text="{Binding CombinedDamageTakenEvadeMod, StringFormat={}{0:N0}}"
                                       Visibility="{Binding ShowBasicCombinedDamageTakenEvadeMod, Source={StaticResource Settings}, Converter={StaticResource VisibilityConverter}}" />
                            <TextBlock Foreground="{DynamicResource LabelTextBrush}"
                                       Text="{Binding CombinedDamageTakenEvadeModAverage, StringFormat={}{0:N0}}"
                                       Visibility="{Binding ShowBasicCombinedDamageTakenEvadeModAverage, Source={StaticResource Settings}, Converter={StaticResource VisibilityConverter}}" />
                        </StackPanel>
                    </DockPanel>
                </DockPanel>
            </Grid>
        </Border>
    </Grid>
</UserControl>